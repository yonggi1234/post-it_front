<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>PMI Board</title>
        <link rel="stylesheet" href="/stylesheets/pmi.css" type="text/css">
    </head>
    <body>
        <canvas id = "canvas" width="1600" height="1000"></canvas>
        <div id="circle1">
            <h3 id="text1">Best</h3>
        </div>
        <div id="circle2"></div>
        <div id="circle3">
            <h3 id="text2">Good</h3>
        </div>
        <div id="circle4">
            <h3 id="text3">Ideas</h3>
        </div>
        <div id="circle5"></div>
        <div id="rectangle"><h3 id="text"><span class="Color">WOW</span> IDEA BOARD</h3></div>

        <div class="container">
        <div class="circle" id="circle"></div>
            <img src="img/blue.png" alt="Draggable Image" class="draggable" id="draggableImage1">
            <img src="img/green.png" alt="Draggable Image" class="draggable" id="draggableImage2">
            <img src="img/pink.png" alt="Draggable Image" class="draggable" id="draggableImage3">
            <img src="img/orange.png" alt="Draggable Image" class="draggable" id="draggableImage4">
            <img src="img/yellow.png" alt="Draggable Image" class="draggable" id="draggableImage5">
        </div>
        
        <script src="js/pmi.js"></script>
        <script src="js/drawImg.js"></script>        
    </body>
